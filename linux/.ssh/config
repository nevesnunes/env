AddKeysToAgent yes
ServerAliveInterval 120
Host *
    ControlMaster auto
    ControlPath ~/.ssh/master-%r@%h:%p.socket
    ControlPersist 30m
Host 192.168.1.5
    HostName 192.168.1.5
    User fn
    Port 22550
    IdentityFile /home/fn/.ssh/mnu2021
Host mnu
    HostName mnu
    User fn
    Port 22550
    IdentityFile /home/fn/.ssh/mnu2021
